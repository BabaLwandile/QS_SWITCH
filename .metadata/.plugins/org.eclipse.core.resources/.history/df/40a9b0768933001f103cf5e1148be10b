#include "motor_control.h"
#include "main.h"

// Motor control directions
#define MOTOR_ANTICLOCKWISE 0
#define MOTOR_CLOCKWISE 1


void ControlMotor(int direction){
    uint32_t startTime=HAL_GetTick();

    if (direction == 1){
	//clockwise
	HAL_GPIO_WritePin(GPIOB, GPIO_, PinState)
    }




}

